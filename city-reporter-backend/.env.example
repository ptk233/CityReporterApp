# Przykładowe zmienne środowiskowe dla Railway/Render

# Spring Boot
SPRING_PROFILES_ACTIVE=prod

# Database (Railway automatycznie ustawia DATABASE_URL)
DATABASE_URL=postgresql://user:password@host:port/database
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your-secure-password

# JWT Secret (wygeneruj: openssl rand -base64 32)
JWT_SECRET=your-very-long-and-secure-secret-key-at-least-32-characters

# Port (Railway automatycznie ustawia)
PORT=8080
